 order.map((orderItem, i) => {\n                if (i === itemIndex){\n                    return {\n                        ...orderItem,\n                        quantity: orderItem.quantity + 1\n                    }\n                }else {\n                   return orderItem\n                }\n\n            })\n            setOrder(newOrder)\n        }\n            setAlertName(item.name)\n\n    }\n\n    const handleBasketShow = () => {\n        setIsBasketShow(!isBasketShow)\n    }\n\n    const removeFromBasket = (itemId) => {\n         const newOrder = order.filter((item) => (item.id !== itemId))\n        setOrder(newOrder)\n    }\n\n    const incQuantity = (itemId) => {\n        const newOrder = order.map((el) => {\n\n                if(el.id === itemId){\n                    const newQuantity = el.quantity + 1\n                    return {\n                        ...el,\n                        quantity: newQuantity\n                    }\n                }else {\n                    return el\n                }\n\n        })\n        setOrder(newOrder)\n    }\n    const decQuantity = (itemId) => {\n        const newOrder = order.map((el) => {\n\n            if(el.id === itemId){\n                const newQuantity = el.quantity - 1\n                return {\n                    ...el,\n                    quantity: newQuantity >= 0? newQuantity: 0\n                }\n            }else {\n                return el\n            }\n\n        })\n        setOrder(newOrder)\n    }\n    const closeAlert = () => {\n        setAlertName('')\n    }\n\n\n    useEffect(()=> {\n        fetch(API_URL, {\n            headers: {\n                Authorization: API_KEY,\n            },\n        })\n            .then(res => res.json())\n            .then(data => {\n            console.log(data)\n               data.featured && setGoods(data.featured);\n                setLoading(false)\n            })\n\n        console.log(goods.length)\n    }, [])\n\n    return (\n        <main className=\"container content\">\n            <Cart quantity={order.length} handleBasketShow={handleBasketShow}/>\n            {loading ? <Preloader/>: <GoodsList goods={goods} addToBasket={addToBasket}/>}\n            {isBasketShow ? <BasketList\n                order={order}\n                handleBasketShow={handleBasketShow}\n                removeFromBasket={removeFromBasket}\n                incQuantity={incQuantity}\n                decQuantity={decQuantity}\n            />: null}\n            {alertName && <Alert name={alertName} closeAlert={closeAlert}/>}\n        </main>\n    )\n}\nexport default Shop","/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/config.js",[],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/Header.jsx",["75","76"],"import React from 'react'\n\nfunction Header() {\n    return (\n        <nav className={'green darken-1'}>\n            <div className=\"nav-wrapper\">\n                <a href=\"#\" className=\"brand-logo\">React Shop</a>\n                <ul id=\"nav-mobile\" className=\"right hide-on-med-and-down\">\n                    <li><a href=\"#\">Repo</a></li>\n\n                </ul>\n            </div>\n        </nav>\n    )\n}\nexport default Header","/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/GoodsItem.jsx",[],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/Cart.jsx",[],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/Goods.jsx",["77"],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/BasketItem.jsx",[],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/BasketList.jsx",[],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/Alert.jsx",["78"],"/home/dm1tro/Desktop/react_2/nepomnyashiy/shop-project/src/components/Preloader.jsx",[],{"ruleId":"79","severity":2,"message":"80","line":15,"column":1,"nodeType":"81","messageId":"82","endLine":15,"endColumn":16},{"